<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="tvshows">
    <name>Anime (In progress)</name>
    <match>all</match>
    <limit>25</limit>
    <rule field="inprogress" operator="true" />
    <rule field="lastplayed" operator="inthelast">12 Months</rule>
    <rule field="tag" operator="contains">Anime</rule>
    <order direction="descending">lastplayed</order>
</smartplaylist>
