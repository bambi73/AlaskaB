          <control type="edit" id="11">
            <include>DialogNetworkSetupGLTemplate</include>
            <label>$LOCALIZE[1024]</label>
          </control>
          <!-- Port -->
          <control type="edit" id="13">
            <include>DialogNetworkSetupGLTemplate</include>
            <label>$LOCALIZE[1013]</label>
